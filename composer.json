{
    "name": "ellipse/binder",
    "type": "composer-plugin",
    "description": "Service provider auto discovery plugin",
    "keywords": [
        "service-provider",
        "package",
        "discovery"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/binder",
    "support": {
        "issues": "https://github.com/ellipsephp/binder/issues",
        "source": "https://github.com/ellipsephp/binder"
    },
    "authors": [
        {
            "name": "Pierre Mallinjoud",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "composer-plugin-api": "^1.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "^1.6",
        "mockery/mockery": "^1.0@alpha",
        "league/flysystem-vfs": "^1.0",
        "composer/composer": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\": "src"
        }
    },
    "extra": {
        "class": "Ellipse\\Binder\\Plugin",
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
