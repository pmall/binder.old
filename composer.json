{
    "name": "ellipse/binder",
    "type": "library",
    "description": "The ellipse framework's utility for service provider auto discovery",
    "keywords": [
        "service-provider",
        "package",
        "discovery"
    ],
    "license": "MIT",
    "homepage": "https://github.com/ellipsephp/binder",
    "support": {
        "issues": "https://github.com/ellipsephp/binder/issues",
        "source": "https://github.com/ellipsephp/binder"
    },
    "authors": [
        {
            "name": "Pierre Mallinjoud",
            "email": "pierre.mallinjoud@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "peridot-php/peridot": "^1.19",
        "peridot-php/leo": "^1.6",
        "mockery/mockery": "^1.0@alpha",
        "league/flysystem-vfs": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Ellipse\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
